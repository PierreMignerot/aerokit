    "# Wall interaction\n",
    "pbw = riem.riemann_pb(q01, q01gh)\n",
    "q02 = pbw.qstarL()\n",
    "print (\"shock compression ratio:\", q02.p/q01.p)\n",
    "print (\"shock Mach number      :\", pbw.left_fastest()/q01.asound())"
