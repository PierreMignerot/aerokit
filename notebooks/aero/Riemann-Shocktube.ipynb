    "# Helium \n",
    "q00 = uq.unsteady_state(rho = Pinit/(r_He*Tisoth),\n",
    "                        u   = 0., \n",
    "                        p   = Pinit, gamma=gam_He)\n",
    "# Riemann problem\n",
    "pb2 = riem.riemann_pb(q11, q00)\n",
    "q12 = pb2.qstarL()\n",
    "q01 = pb2.qstarR()\n",
    "print (\"shock compression ratio:\", q01.p/q00.p)\n",
    "print (\"interface speed        :\", q12.u)"
   ]
